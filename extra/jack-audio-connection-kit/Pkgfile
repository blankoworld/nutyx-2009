name=jack-audio-connection-kit
version=0.109.2
release=1
source=(http://mesh.dl.sourceforge.net/sourceforge/jackit/$name-$version.tar.gz)

build() {
	cd $name-$version
	./configure --prefix=/usr
	make
	make DESTDIR=$PKG install
}
