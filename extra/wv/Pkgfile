name=wv
version=1.2.4
release=1
source=(http://downloads.sourceforge.net/$name/$name-$version.tar.gz)

build() {
		cd $name-$version
                ./configure --prefix=/usr --without-docs
		make
		make DESTDIR=$PKG install
}
