<?xml version="1.0" encoding="ISO-8859-1"?> <!-- -*- SGML -*- -->
<match key="info.capabilities" contains="input.keyboard">
        <merge key="input.x11_driver" type="string">keyboard</merge>
        <merge key="input.xkb.layout" type="string">fr</merge>
