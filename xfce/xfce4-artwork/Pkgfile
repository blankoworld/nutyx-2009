# Depends on: 

name=xfce4-artwork
version=0.1
release=5
source=(http://download.berlios.de/xfce-goodies/$name-$version.tar.gz)

build() {
cd $name-$version
	./configure --prefix=/usr \
	--sysconfdir=/etc \
	--libexecdir=/usr/lib/xfce4 \
	--localstatedir=/var \
	--disable-static
	make
	make DESTDIR=$PKG install
}

