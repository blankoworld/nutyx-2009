name=xbindkeys
version=1.8.2
release=1
source=(http://hocwp.free.fr/xbindkeys/$name-$version.tar.gz)

build() {
cd $name-$version
./configure --prefix=/usr --sysconfdir=/etc
make
make DESTDIR=$PKG install
}
